		"cypress": "^12.2.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"google-auth-library": "^8.7.0",
		"husky": "^8.0.1",
		"js-yaml": "^4.1.0",
